{
  "name": "git-diary",
  "displayName": "Git Diary",
  "description": "Automatically tracks code changes and commits them to a private GitHub diary repository",
  "version": "0.1.0",
  "publisher": "YourName",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-diary.changeInterval",
        "title": "Git Diary: Change Commit Interval",
        "category": "Git Diary"
      }
    ],
    "configuration": {
      "title": "Git Diary",
      "properties": {
        "git-diary.commitInterval": {
          "type": "number",
          "default": 30,
          "minimum": 1,
          "description": "Automatic commit interval in minutes"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0"
  },
  "dependencies": {
    "axios": "^1.8.1",
    "lodash": "^4.17.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/git-diary-extension"
  },
  "icon": "images/icon.png",
  "keywords": [
    "git",
    "diary",
    "productivity",
    "time-tracking",
    "automation"
  ]
}